	<target name="generate-commit-id">
    <echo message="Running getcommitID.bat to generate CommitID.java..."/>
	<exec executable="getcommitID.bat" dir="." failonerror="true"/>
	</target>
	<target depends="init,generate-commit-id" name="build-project">
        <echo message="${ant.project.name}: ${ant.file}"/>
        <javac debug="true" debuglevel="${debuglevel}" destdir="bin" includeantruntime="false" source="${source}" target="${target}">
            <src path="src"/>
            <exclude name="dependencies/"/>
            <classpath refid="RetroLemmini.classpath"/>
        </javac>
    </target>
    <target description="Build all projects which reference this project. Useful to propagate changes." name="build-refprojects"/>
